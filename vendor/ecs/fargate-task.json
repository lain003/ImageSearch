{
  "executionRoleArn": "arn:aws:iam::620988379686:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "environment": [
        {
          "name": "cluster.name",
          "value": "develop-cluster"
        },
        {
          "name": "discovery.type",
          "value": "single-node"
        }
      ],
      "image": "lain003/release_elastic_search",
      "name": "elastic_search",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "image_search",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "elastic",
          "awslogs-create-group": "true"
        }
      }
    },
    {
      "portMappings": [
        {
          "hostPort": 3000,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "command": [
        "bash",
        "-c",
        "-i",
        "cd /home/imagesearch && bundle exec rails s -e production"
      ],
      "repositoryCredentials": {
        "credentialsParameter": "arn:aws:secretsmanager:ap-northeast-1:620988379686:secret:DockerHubLogin-NtgZPU"
      },
      "environment": [
        {
          "name": "RAILS_LOG_TO_STDOUT",
          "value": "true"
        }
      ],
      "image": "lain003/release_image_search",
      "name": "image_search",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "image_search",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "rails",
          "awslogs-create-group": "true"
        }
      }
    },
    {
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "image": "lain003/release_nginx",
      "name": "nginx",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "image_search",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "nginx",
          "awslogs-create-group": "true"
        }
      }
    }
  ],
  "family": "customImageSearch",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "8192"
}